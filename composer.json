{
    "dev-master": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-redis": "*",
        "brick/varexporter": "*",
        "coduo/php-matcher": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^2.0.1",
        "doctrine/collections": "^1.6",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "haydenpierce/class-finder": "*",
        "itspire/monolog-loki": "^2.0",
        "lexik/jwt-authentication-bundle": "^2.19",
        "micoli/docker-php-api-client": "^4.1",
        "micoli/elql": "^0.7.0",
        "mockery/mockery": "^1.4",
        "overblog/graphql-bundle": "^1.7.0",
        "php-http/guzzle7-adapter": "1.0.0",
        "php-http/logger-plugin": "^1.2",
        "php-http/socket-client": "*",
        "phpseclib/phpseclib": "^3.0",
        "ramsey/uuid": "^4.1",
        "supervisorphp/supervisor": "^5.1",
        "symfony/browser-kit": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/doctrine-messenger": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/expression-language": "7.2.*",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "7.2.*",
        "symfony/mercure-bundle": "^0.3.9",
        "symfony/messenger": "7.2.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "7.2.*",
        "symfony/property-info": "7.2.*",
        "symfony/redis-messenger": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "7.2.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v3.20.0",
        "jane-php/json-schema": "^7.4",
        "jane-php/open-api-3": "^7.4",
        "phpstan/phpstan-symfony": "^1.4",
        "phpunit/phpunit": "^10.2",
        "rector/rector": "*",
        "symfony/phpunit-bridge": "^7.2",
        "symfony/stopwatch": "7.2.*",
        "symfony/web-profiler-bundle": "7.2.*",
        "tomasvotruba/cognitive-complexity": "*"
    },
    "minimum-stability": "alpha",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "php-http/discovery": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Common\\": "lib/Common/src",
            "Micoli\\Trail\\": "lib/Trail/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/",
            "App\\Common\\": "lib/Common/src",
            "Tests\\": "tests/",
            "Tests\\Common\\": "lib/Common/tests/",
            "Micoli\\Trail\\tests\\": "lib/Trail/tests"
        }
    },

    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-autoload-dump": [
            "@auto-scripts"
        ]
    }
}
